{
  "name": "after-effects",
  "version": "1.0.1a10",
  "description": "Send instructions to the After Effects scripting environment from node.js. ",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/jasmine-ts ",
    "b::AEHelper": "node src/Helpers/AEHelper.js",
    "b::IOHelper": "node src/Helpers/IOHelper.js",
    "b::CompHelper": "node src/Helpers/CompHelper.js",
    "c::AEHelper": "node_modules/.bin/tsc --p  .  src/helpers/AEHelper.ts",
    "c::IOHelper": "node_modules/.bin/tsc src/helpers/IOHelper.ts",
    "c::CompHelper" : "node_modules/.bin/tsc src/helpers/CompHelper.ts " , 
    "c_" :"c::AEHelper &  c::IOHelper & c::CompHelper ",
    "b_" : "b::AEHelper &  b::IOHelper & b::CompHelper ",
    "build": "npm run c_ & npm run b_ && npm pack"
  },
  "keywords": [
    "after",
    "effects"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BenZed/after-effects"
  },
  "author": "Ben Gaumond",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-es5-property-mutators": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "escape-string-applescript": "^2.0.0",
    "is-explicit": "^1.2.2",
    "uglify-js": "^2.6.2",
    "uuid": "^2.0.1",
    "types-for-adobe": "^1.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/jasmine": "^3.4.6",
    "@types/node": "^12.12.12",
    "chai": "^4.2.0",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
